    <%= f.radio_button :genre, "ラーメン", class: "form-check-input",checked: @search_params[:genre]=="ラーメン" %>
    <%= f.label :ラーメン, class: "form-check-label search-label" %>
  </div>
  <div class="form-check form-check-inline form-search-check"">
    <%= f.radio_button :genre, "つけ麺", class: "form-check-input",checked: @search_params[:genre]=="つけ麺" %>
    <%= f.label :つけ麺, class: "form-check-label search-label" %>
  </div>
  <div class=" form-check form-check-inline form-search-check"">
    <%= f.radio_button :genre, "油そば", class: "form-check-input",checked: @search_params[:genre]=="油そば" %>

    <%= f.radio_button :taste, "とてもあっさり", class: "form-check-input",checked: @search_params[:taste]=="とてもあっさり" %>
    <%= f.label :とてもあっさり, class: "form-check-label search-label" %>
  </div>
  <div class="form-check form-check-inline form-search-check">
